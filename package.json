{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "serve": "concurrently \"php artisan serve --host=127.0.0.1 --port=8001\" \"php artisan queue:work --daemon --tries=3 --timeout=300\"",
        "serve:verbose": "concurrently --names \"SERVER,QUEUE\" --prefix-colors \"cyan,yellow\" \"php artisan serve --host=127.0.0.1 --port=8001\" \"php artisan queue:work --verbose --tries=3 --timeout=300\"",
        "serve:dev": "concurrently --names \"SERVER,QUEUE,VITE\" --prefix-colors \"cyan,yellow,green\" \"php artisan serve --host=127.0.0.1 --port=8001\" \"php artisan queue:work --daemon --tries=3 --timeout=300\" \"vite\""
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7"
    },
    "dependencies": {
        "frappe-gantt": "^1.0.4"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
